                <owl:onProperty rdf:resource="&zigbee;clusterID"/>
                <owl:hasValue>0b02</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&zigbee;includeFunctionality"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&dogont;AlertNotificationFunctionality"/>
                            <rdf:Description rdf:about="&dogont;EventNotificationFunctionality"/>
                            <rdf:Description rdf:about="&dogont;EventsAndAlertsNotificationFunctionality"/>
                            <rdf:Description rdf:about="&dogont;EventsAndAlertsQueryFunctionality"/>
                            <rdf:Description rdf:about="&dogont;MultipleAlertNotificationFunctionality"/>
                            <rdf:Description rdf:about="&dogont;MultipleEventNotificationFunctionality"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
