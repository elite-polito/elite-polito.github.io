    <!-- http://elite.polito.it/ontologies/zigbee.owl#EN50523ApplianceControl -->

    <owl:Class rdf:about="&zigbee;EN50523ApplianceControl">
        <rdfs:subClassOf rdf:resource="&zigbee;EnergyAtHome_Cluster"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&zigbee;includeFunctionality"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&dogont;CycleTimingControlFunctionality"/>
                            <rdf:Description rdf:about="&dogont;CycleTimingQueryFunctionality"/>
                            <rdf:Description rdf:about="&dogont;EnableDisableGasFunctionality"/>
                            <rdf:Description rdf:about="&dogont;StartStopPauseFunctionality"/>
                            <rdf:Description rdf:about="&dogont;SuperCoolingFunctionality"/>
                            <rdf:Description rdf:about="&dogont;SuperFreezingFunctionality"/>
                            <rdf:Description rdf:about="&dogont;SuperHeatingFunctionality"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&zigbee;clusterID"/>
                <owl:hasValue>001b</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


